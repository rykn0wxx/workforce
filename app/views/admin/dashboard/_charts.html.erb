<div class="well">
  <div id="chart1" style="min-width: 300px; height: 400px; margin: 0 auto"></div>
</div>

<%= javascript_tag do %>
  $(function() {
    var usr = <%= raw projs %>
    Highcharts.chart('chart1', {
      chart: {
        type: 'column'
      },
      title: {
        text: null
      },
      xAxis: {
        type: 'category'
      },
      yAxis: {
        min: 0
      },
      series: [{
        name: 'Sample',
        data: [
          ['Shanghai', 23.7],
          ['Lagos', 16.1],
           ['Karachi', 14.0]
        ]
      }]
    });
    console.log(usr);
  });
<% end %>
