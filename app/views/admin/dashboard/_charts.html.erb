<div class="well">
  <div id="chart1" style="min-width: 100%; height: 400px; margin: 0 auto"></div>
</div>

<%= javascript_tag do %>
  $(function() {
    $('#chart1').highcharts({
      chart: {
        type: 'spline'
      },
      title: {
        text: 'asd'
      },
      xAxis: {
        type: 'datetime'
      },
      series: [{
        name: 'AA',
        pointInterval: <%= 1.day * 1000 %>,
        pointStart: <%= 1.month.ago.to_i * 1000 %>,
        data: <%= z_chart(1.month.ago, Date.today, PhoneFact) %>
      }]
    });
    setTimeout(function() {
      var hc = $('#chart1').highcharts();
      hc.reflow();
    },300);
  });
<% end %>
